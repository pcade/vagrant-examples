#Checks
curl http://localhost
curl http://localhost:80
curl http://localhost:8080
curl http://localhost:8888

#host
# Создание конфигурационного файла SSH
vagrant ssh-config web2 > ssh_config
# Проброс порта с VM на хост
ssh -F ssh_config -L 8080:localhost:80 web2

# Проброс порта с VM на хост с помощью SOCKS5 прокси
ssh -F ssh_config -D 1080 web2
curl --socks5 localhost:1080 http://google.com


# VM
sudo apt update && sudo apt install -y apache2
sudo systemctl enable --now apache2

# Проброс порта с VM на хост
ssh -R 8888:localhost:80 u@192.168.122.1

sudo tcpdump
sudo tcpdump -ni enp0s3 tcp port 80 -c 5
